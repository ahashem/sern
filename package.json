{
  "name": "sern",
  "version": "0.4.0",
  "description": "RESTful API Nodejs based on Swagger, Sequelize, Express, React ",
  "main": "entry.js",
  "scripts": {
    "start": "./node_modules/.bin/nodemon entry.js",
    "cleanbuild": "npm run clean && cross-env NODE_ENV=production webpack --config ./config/webpack.config.js -p",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js -p",
    "test": "./node_modules/.bin/mocha --reporter spec test/**.js --exit",
    "codecov": "npm run rmrf:coverage && istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec test/**.js  --exit && codecov",
    "rmrf:coverage": "rimraf ./coverage",
    "lint": "eslint . --ext .js",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahashem/sern.git"
  },
  "author": "Ahmed Hashem",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ahashem/sern/issues"
  },
  "homepage": "https://github.com/ahashem/sern#readme",
  "keywords": [
    "swagger",
    "swagger-api",
    "rest-swagger",
    "rest-api",
    "RESTful",
    "REST",
    "nodejs",
    "nodejs-api",
    "rest",
    "passport",
    "react",
    "express"
  ],
